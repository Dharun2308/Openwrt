
config system
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option hostname 'Arca'
	option zonename 'Asia/Kolkata'
	option timezone 'IST-5:30'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config arca 'custom'
	option speedboost '0'
	option loadbalancing '0'
        option static_load '0'
        option lb_change '0'
        option internet '0' 
        option wireless_set '0'

config arca 'vpn'
        option vpn '0'
        option name '---'
        option vpn1 '0'
        option vpn1_name 'JAITRA1'
        option vpn1_username 'admin'
        option vpn2_username 'admin'
        option vpn1_type 'Config File'
        option vpn2 '0'
        option vpn2_name 'JAITRA2'
        option vpn2_type 'Config File'

config arca 'pc'
        option parental '0'
        option parental_change '0'
        option parental_u '0'
        option parental_ua '0'
        option parental_a '0'

config arca 'nas'
        option nas_change '0'
        option nas '0'

config arca 'wan_ip'
        option eth1 'Not connected'
        option eth2 'Not connected'
        option eth3 'Not connected'
        option eth4 'Not connected'
        option eth5 'Not connected'
        option eth6 'Not connected'
        option wlan0 'Not connected'

config arca 'wan_name'
        option eth1 '---'
        option eth2 '---'
        option eth3 '---'
        option eth4 '---'
        option eth5 '---'
        option eth6 '---'
        option wlan0 '---'

config arca 'wan_internet'
        option eth1 '0'
        option eth2 '0'
        option eth3 '0'
        option eth4 '0'
        option eth5 '0'
        option eth6 '0'
        option wlan0 '0'