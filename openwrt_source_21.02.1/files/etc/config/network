
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option multipath 'off'

config globals 'globals'
	option ula_prefix 'fd09:fbe2:fb95::/48'
	option multipath 'enable'
	option mptcp_path_manager 'fullmesh'
	option mptcp_scheduler 'blest'
	option congestion 'cubic'
	option mptcp_checksum '0'
	option mptcp_debug '0'
	option mptcp_syn_retries '2'
	option mptcp_fullmesh_num_subflows '1'
	option mptcp_fullmesh_create_on_err '1'
	option mptcp_ndiffports_num_subflows '1'
	option mptcp_rr_cwnd_limited 'Y'
	option mptcp_rr_num_segments '1'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth0'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option delegate '0'
	option multipath 'off'

config interface 'wan'
	option device 'eth1'
	option multipath 'on'
	option proto 'dhcp'
	option metric '1'
	option disabled '1'

config interface 'tun0'
	option proto 'dhcp'
	option device 'tun0'
	option multipath 'off'

config interface 'wan2'
	option proto 'dhcp'
	option device 'eth2'
	option multipath 'on'
	option metric '2'
	option disabled '0'

config interface 'wan3'
	option proto 'dhcp'
	option device 'eth3'
	option multipath 'on'
	option metric '3'
	option disabled '0'

config interface 'wan4'
	option proto 'dhcp'
	option device 'eth4'
	option multipath 'on'
	option metric '4'
	option disabled '0'

config interface 'wan5'
	option proto 'dhcp'
	option device 'eth5'
	option multipath 'on'
	option metric '5'
	option disabled '0'

config interface 'wan6'
	option proto 'dhcp'
	option device 'eth6'
	option multipath 'on'
	option metric '6'
	option disabled '0'

config interface 'wwan'
	option proto 'dhcp'
	option multipath 'on'
	option metric '7'
	option disabled '0'

config interface 'docker'
	option type 'bridge'
        option proto 'static'
        option force_link '1'
        option bridge_empty '1'
        option ipaddr '10.21.0.1'
        option netmask '255.255.255.0'

config device
	option type 'bridge'
	option name 'docker0'

